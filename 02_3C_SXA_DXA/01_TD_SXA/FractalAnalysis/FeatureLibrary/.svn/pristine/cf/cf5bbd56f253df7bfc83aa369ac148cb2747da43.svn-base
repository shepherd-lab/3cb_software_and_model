function output = diagM(calcParam, inputParam) %#ok<INUSD>
%diagM computes Diagonal Moment
%Notation described in 'Feature Definition and Algorithm.docx'
n = size(calcParam.comat, 1);
output = 0;
for i = 1:n
    for j = 1:n
        output = output + sqrt(0.5 * abs(i-j) * calcParam.comat(i, j));
    end
end
end