function output = entropy(calcParam, inputParam) %#ok<INUSD>
%entropy computes Entropy
%Notation described in 'Feature Definition and Algorithm.docx'
n = size(calcParam.comat, 1);
output = 0;
for i = 1:n
    for j = 1:n
        if calcParam.comat(i, j) ~= 0
            output = output + calcParam.comat(i, j)*log(calcParam.comat(i, j));
        end
    end
end
output = -output;
end