function [output,outputFeature_id, outputMaskNum]=evaluatesubfunc(calcParam,inputParam)
%evaluates all the subfunction calls and concatenates them into arrays.
output=[];
outputNames={};
for i=1:size(inputParam.sub_func,2)
    fcnPointer=inputParam.sub_func{i}.processFcn;
    [newOutput,newOutputFeature_id, new]=fcnPointer(calcParam,inputParam.sub_func{i});
    output=[output,newOutput]; %#ok<AGROW>
    outputNames=[outputNames,newOutputNames]; %#ok<AGROW>
end
end