function output = shortRunsEmph(calcParam, inputParam) %#ok<INUSD>
%calculate short runs emphasis from a run-length matrix
[m, n] = size(calcParam.rlMatObj.matrix);
SREsum = 0;
for i = 1:m
    for j = 1:n
        SREsum = SREsum + calcParam.rlMatObj.matrix(i, j)/(j^2);
    end
end
output = SREsum/calcParam.NOR;
end
