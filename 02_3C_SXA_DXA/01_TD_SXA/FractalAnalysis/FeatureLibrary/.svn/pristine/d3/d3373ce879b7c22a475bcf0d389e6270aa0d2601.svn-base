function output = triangSymm(calcParam, inputParam) %#ok<INUSD>
%triangSymm computes Triangular Symmetry
%Notation described in 'Feature Definition and Algorithm.docx'
n = size(calcParam.comat, 1);
output = 0;
for i = 1:n
    for j = 1:n
        output = output + abs(calcParam.comat(i, j) - calcParam.comat(j, i));
    end
end
end