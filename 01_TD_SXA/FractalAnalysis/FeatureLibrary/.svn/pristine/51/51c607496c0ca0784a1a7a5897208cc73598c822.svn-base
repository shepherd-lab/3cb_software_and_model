function output = prodM(calcParam, inputParam) %#ok<INUSD>
%prodM computes Product Moment
%Notation described in 'Feature Definition and Algorithm.docx'
u = mean(calcParam.comat(:));
n = size(calcParam.comat, 1);
output = 0;
for i = 1:n
    for j = 1:n
        output = output + (i - u) * (j - u) * calcParam.comat(i, j);
    end
end
end