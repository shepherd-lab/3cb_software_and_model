function output = sumEntropy(calcParam, inputParam) %#ok<INUSD>
%sumEntropy computes Sum Entropy
%Notation described in 'Feature Definition and Algorithm.docx'
n = length(calcParam.sumVec);
output = 0;
for i = 1:n
    if calcParam.sumVec(i) ~= 0
        output = output + calcParam.sumVec(i)*log(calcParam.sumVec(i));
    end
end
output = -output;
end