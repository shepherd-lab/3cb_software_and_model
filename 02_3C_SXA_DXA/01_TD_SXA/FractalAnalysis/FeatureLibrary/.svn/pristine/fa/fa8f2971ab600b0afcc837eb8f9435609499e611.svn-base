function output = diffEntropy(calcParam, inputParam) %#ok<INUSD>
%diffEntropy computes Difference Entropy
%Notation described in 'Feature Definition and Algorithm.docx'
n = length(calcParam.diffVec);
output = 0;
for i = 1:n
    if calcParam.diffVec(i) ~= 0
        output = output + calcParam.diffVec(i)*log(calcParam.diffVec(i));
    end
end
output = -output;
end