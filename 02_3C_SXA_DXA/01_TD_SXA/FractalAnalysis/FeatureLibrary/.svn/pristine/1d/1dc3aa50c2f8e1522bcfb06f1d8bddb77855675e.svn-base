function [output outputFeature_id]= laws(imageObj, maskObj, inputParam)
%This function calculate Laws features.
%inputParams is a cell array
%inputParam{1}: vertical filter (string, ie L5)
%inputParam{2}: horizontal filter (string, ie E5)
%inputParam{3}: order of moment (double, ie 2)

filVer = inputParam.main_func.parameter_1;
filHor = inputParam.main_func.parameter_2;
[imFiltered, mask] = applyLawsFilter(imageObj, maskObj, filVer, filHor);
roiIdx = mask ~= 0;
mOrder = inputParam.main_func.parameter_3;
output = moment(imFiltered(roiIdx), mOrder);
outputFeature_id=inputParam.sub_func{1}.feature_id;
end
