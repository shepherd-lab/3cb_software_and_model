function output = longRunsEmph(calcParam, inputParam) %#ok<INUSD>
%calculate the long run emphasis based on the run-length matrix
[m, n] = size(calcParam.rlMatObj.matrix);
LREsum = 0;
for i = 1:m
    for j = 1:n
        LREsum = LREsum + (j^2)*calcParam.rlMatObj.matrix(i, j);
    end
end
output = LREsum/calcParam.NOR;
end
