function output = correlation(calcParam, inputParam) %#ok<INUSD>
%correlation computes Correlation
%Notation described in 'Feature Definition and Algorithm.docx'
Cx = sum(calcParam.comat, 2);
Cy = sum(calcParam.comat, 1);
Mx = mean(Cx);
My = mean(Cy);
Sx = std(Cx);
Sy = std(Cy);
n = size(calcParam.comat, 1);
output = 0;
for i = 1:n
    for j = 1:n
        output = output + i*j*calcParam.comat(i, j) - Mx*My;
    end
end
output = output/(Sx*Sy);
end