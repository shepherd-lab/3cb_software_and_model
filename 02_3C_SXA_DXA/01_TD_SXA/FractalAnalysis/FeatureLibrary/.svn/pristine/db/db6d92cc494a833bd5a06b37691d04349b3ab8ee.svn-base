function output=fft_fd(calcParam,inputParam) %#ok<INUSD>
maxf = max(calcParam.f);
for k=1:maxf
    r_index = find(calcParam.f==k);
    if ~isempty(r_index)
        Pfft_vect(k) = mean(calcParam.P_vect(r_index)); %#ok<AGROW>
    end
end
xfm = 1:maxf;
logf = log10(xfm);
logP = log10(Pfft_vect);
findex = find(logf>1.2 & logf<2.7);
pf = polyfit(logf(findex)',logP(findex)',1);
output=-pf(1);
end
