function output = highGlevelRunEmph(calcParam, inputParam) %#ok<INUSD>
%calculate the high gray-level run emphasis based on the run-length matrix
[m, n] = size(calcParam.rlMatObj.matrix);
glevelMin = calcParam.rlMatObj.glevelMin;
HGREsum = 0;
for i = 1:m
    glevel = glevelMin + i - 1;
    for j = 1:n
        HGREsum = HGREsum + (glevel^2)*calcParam.rlMatObj.matrix(i, j);
    end
end
output = HGREsum/calcParam.NOR;
end
